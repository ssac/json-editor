{
  "name": "@ssac/json-editor",
  "version": "1.0.0",
  "description": "Utilities to manipulate .json file.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssac/json-editor.git"
  },
  "keywords": [
    "json",
    "editor",
    "utility"
  ],
  "author": "ssac",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ssac/json-editor/issues"
  },
  "homepage": "https://github.com/ssac/json-editor#readme",
  "files": [
    "/dist"
  ],
  "dependencies": {
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
